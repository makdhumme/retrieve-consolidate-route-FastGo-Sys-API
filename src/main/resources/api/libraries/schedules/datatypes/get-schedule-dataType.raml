#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime:
      type: string
      required: true
      example: "2019-04-30T16:41:41.0902"
    fromLocation:
      type: string
      required: true
      example: "BEN"
    toLocation:
      type: string
      required: true
      example: "kLG"
    availableSeats:
      type: number
      required: true
      example: 20